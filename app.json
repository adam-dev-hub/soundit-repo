{
  "expo": {
    "name": "SoundIt",
    "slug": "soundit",
    "scheme": "com.soundit.app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.soundit.app",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK"
      ]
     
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soundit.app",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-av",
      [
        "expo-system-ui",
        {
          "androidNavigationBarColor": "#1a1a1a"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a6a33184-db92-435b-abd5-cef7e48837f2"
      }
    }
  }
}